{

  "manifest_version": 2,
  "name": "ReadMoreRemover",
  "version": "2.3",
  "description": "Triggers Continue Reading links on www.huffingtonpost.com for Firefox on Android",
  "icons": {
    "48": "icons/remover.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.huffingtonpost.com/*",
        "*://*.huffpost.com/*",
        "*://*.huffingtonpost.co.uk/*",
        "*://*.aol.com/*"
      ],
      "js": ["remover.js"]
    }
  ]

}
